{
  "name": "xynteo-chat",
  "version": "1.0.0",
  "private": true,
  "description": "Xynteo chat",
  "scripts": {
    "deploy:test": "SERVICE=chat-test npm run deploy",
    "deploy": "npm run build && ecs-updater",
    "build": "npm install && tslint --project . && tsc",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/app/app.ts",
    "test": "mocha -r ts-node/register src/test/**/*.ts"
  },
  "devDependencies": {
    "@decorators/di": "^1.0.1",
    "@types/chai": "^4.1.0",
    "@types/express": "^4.11.0",
    "@types/mocha": "^2.2.46",
    "@types/mongodb": "^3.0.1",
    "@types/node": "^7.0.5",
    "@types/socket.io": "^1.4.31",
    "@types/winston": "^2.3.7",
    "chai": "^4.1.2",
    "ecs-updater": "^0.3.6",
    "mocha": "^4.1.0",
    "nodemon": "^1.14.11",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "@decorators/socket": "^3.0.1",
    "express": "^4.16.2",
    "mongodb": "^3.0.1",
    "socket.io": "^2.0.4",
    "socketio-jwt": "^4.5.0",
    "winston": "^2.4.0"
  }
}
